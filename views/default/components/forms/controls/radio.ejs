<%layout(id)%>
<!-- begin:: Content -->
<div class="k-content	k-grid__item k-grid__item--fluid k-grid k-grid--hor" id="k_content">

    <!-- begin:: Content Head -->
    <div class="k-content__head	k-grid__item">
        <div class="k-content__head-main">
            <h3 class="k-content__head-title">Radios</h3>
            <div class="k-content__head-breadcrumbs">
                <a href="#" class="k-content__head-breadcrumb-home"><i class="flaticon2-shelter"></i></a>
                <span class="k-content__head-breadcrumb-separator"></span>
                <a href="" class="k-content__head-breadcrumb-link">Extended</a>
                <span class="k-content__head-breadcrumb-separator"></span>
                <a href="" class="k-content__head-breadcrumb-link">Forms</a>
                <span class="k-content__head-breadcrumb-separator"></span>
                <a href="" class="k-content__head-breadcrumb-link">Controls</a>
                <span class="k-content__head-breadcrumb-separator"></span>
                <a href="" class="k-content__head-breadcrumb-link">Radio</a>

                <!-- <span class="k-content__head-breadcrumb-link k-content__head-breadcrumb-link--active">Active link</span> -->
            </div>
        </div>
        <div class="k-content__head-toolbar">
            <div class="k-content__head-wrapper">
                <div class="dropdown dropdown-inline k-hide" data-toggle="k-tooltip" title="Quick actions" data-placement="left">
                    <button type="button" class="btn btn-sm btn-elevate btn-danger btn-bold btn-upper dropdown-toggle-" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        New
                    </button>
                    <div class="dropdown-menu dropdown-menu-right">
                        <a class="dropdown-item" href="#"><i class="la la-plus"></i> New Report</a>
                        <a class="dropdown-item" href="#"><i class="la la-user"></i> Add Customer</a>
                        <a class="dropdown-item" href="#"><i class="la la-cloud-download"></i> New Download</a>
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item" href="#"><i class="la la-cog"></i> Settings</a>
                    </div>
                </div>
                <a href="#" class="btn btn-sm btn-elevate btn-brand" id="k_dashboard_daterangepicker" data-toggle="k-tooltip" title="Select dashboard daterange" data-placement="left">
                    <span class="k-opacity-7" id="k_dashboard_daterangepicker_title">Today</span>&nbsp;
                    <span class="k-font-bold" id="k_dashboard_daterangepicker_date">Aug 16</span>
                    <i class="flaticon-calendar-with-a-clock-time-tools k-padding-l-5 k-padding-r-0"></i>
                </a>
            </div>
        </div>
    </div>

    <!-- end:: Content Head -->

    <!-- begin:: Content Body -->
    <div class="k-content__body	k-grid__item k-grid__item--fluid" id="k_content_body">
        <div class="row">
            <div class="col-md-6">

                <!--begin::Portlet-->
                <div class="k-portlet">
                    <div class="k-portlet__head">
                        <div class="k-portlet__head-label">
                            <h3 class="k-portlet__head-title">Basic Radio</h3>
                        </div>
                    </div>
                    <div class="k-portlet__body">

                        <!--begin::Form-->
                        <form class="k-form">
                            <div class="form-group">
                                <label>Default Radioes</label>
                                <div class="k-radio-list">
                                    <label class="k-radio">
                                        <input type="radio" name="radio1"> Default
                                        <span></span>
                                    </label>
                                    <label class="k-radio k-radio--disabled">
                                        <input type="radio" disabled name="radio1"> Disabled
                                        <span></span>
                                    </label>
                                    <label class="k-radio">
                                        <input type="radio" checked="checked" name="radio1"> Checked
                                        <span></span>
                                    </label>
                                </div>
                            </div>
                            <div class="form-group">
                                <label>Inline Radioes</label>
                                <div class="k-radio-inline">
                                    <label class="k-radio">
                                        <input type="radio" name="radio2"> Option 1
                                        <span></span>
                                    </label>
                                    <label class="k-radio">
                                        <input type="radio" name="radio2"> Option 2
                                        <span></span>
                                    </label>
                                    <label class="k-radio">
                                        <input type="radio" name="radio2"> Option 3
                                        <span></span>
                                    </label>
                                </div>
                                <span class="form-text text-muted">Some help text goes here</span>
                            </div>
                        </form>

                        <!--end::Form-->
                    </div>
                </div>

                <!--end::Portlet-->

                <!--begin::Portlet-->
                <div class="k-portlet">
                    <div class="k-portlet__head">
                        <div class="k-portlet__head-label">
                            <h3 class="k-portlet__head-title">Horizontal Form</h3>
                        </div>
                    </div>
                    <div class="k-portlet__body">

                        <!--begin::Form-->
                        <form class="k-form">
                            <div class="form-group row">
                                <label class="col-3 col-form-label">Radioes</label>
                                <div class="col-9">
                                    <div class="k-radio-list">
                                        <label class="k-radio">
                                            <input type="radio" name="radio3"> Option 1
                                            <span></span>
                                        </label>
                                        <label class="k-radio">
                                            <input type="radio" name="radio3"> Option 2
                                            <span></span>
                                        </label>
                                        <label class="k-radio">
                                            <input type="radio" checked="checked" name="radio3"> Checked
                                            <span></span>
                                        </label>
                                        <label class="k-radio k-radio--disabled">
                                            <input type="radio" disabled name="radio3"> Disabled
                                            <span></span>
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label class="col-3 col-form-label">Inline Radioes</label>
                                <div class="col-9">
                                    <div class="k-radio-inline">
                                        <label class="k-radio">
                                            <input type="radio" name="radio4"> Option 1
                                            <span></span>
                                        </label>
                                        <label class="k-radio">
                                            <input type="radio" checked="checked" name="radio4"> Option 2
                                            <span></span>
                                        </label>
                                        <label class="k-radio">
                                            <input type="radio" name="radio4"> Option 3
                                            <span></span>
                                        </label>
                                    </div>
                                    <span class="form-text text-muted">Some help text goes here</span>
                                </div>
                            </div>
                        </form>

                        <!--end::Form-->
                    </div>
                </div>

                <!--end::Portlet-->
            </div>
            <div class="col-md-6">

                <!--begin::Portlet-->
                <div class="k-portlet">
                    <div class="k-portlet__head">
                        <div class="k-portlet__head-label">
                            <h3 class="k-portlet__head-title">Color Options</h3>
                        </div>
                    </div>
                    <div class="k-portlet__body">

                        <!--begin::Form-->
                        <form class="k-form">
                            <div class="form-group">
                                <label>Default Radioes</label>
                                <div class="k-radio-list">
                                    <label class="k-radio">
                                        <input type="radio" name="radio5"> Default
                                        <span></span>
                                    </label>
                                    <label class="k-radio">
                                        <input type="radio" checked="checked" name="radio5"> Checked
                                        <span></span>
                                    </label>
                                    <label class="k-radio k-radio--disabled">
                                        <input type="radio" disabled="disabled" name="radio5"> Disabled
                                        <span></span>
                                    </label>
                                    <label class="k-radio k-radio--success" name="radio5">
                                        <input type="radio"> Success state
                                        <span></span>
                                    </label>
                                    <label class="k-radio k-radio--brand" name="radio5">
                                        <input type="radio"> Brand state
                                        <span></span>
                                    </label>
                                </div>
                                <span class="form-text text-muted">Some help text goes here</span>
                            </div>
                            <div class="form-group">
                                <label>Bold Radioes</label>
                                <div class="k-radio-list">
                                    <label class="k-radio k-radio--bold">
                                        <input type="radio" name="radio6"> Default
                                        <span></span>
                                    </label>
                                    <label class="k-radio k-radio--bold">
                                        <input type="radio" checked="checked" name="radio6"> Checked
                                        <span></span>
                                    </label>
                                    <label class="k-radio k-radio--bold k-radio--disabled">
                                        <input type="radio" disabled="disabled" name="radio6"> Disabled
                                        <span></span>
                                    </label>
                                    <label class="k-radio k-radio--bold k-radio--success">
                                        <input type="radio" name="radio6"> Success state
                                        <span></span>
                                    </label>
                                    <label class="k-radio k-radio--bold k-radio--brand">
                                        <input type="radio" name="radio6"> Brand state
                                        <span></span>
                                    </label>
                                </div>
                                <span class="form-text text-muted">Some help text goes here</span>
                            </div>
                            <div class="form-group">
                                <label>Solid Radioes</label>
                                <div class="k-radio-list">
                                    <label class="k-radio k-radio--solid">
                                        <input type="radio" name="radio7"> Default
                                        <span></span>
                                    </label>
                                    <label class="k-radio k-radio--solid">
                                        <input type="radio" checked="checked" name="radio7"> Checked
                                        <span></span>
                                    </label>
                                    <label class="k-radio k-radio--solid k-radio--disabled">
                                        <input type="radio" disabled="disabled" name="radio7"> Disabled
                                        <span></span>
                                    </label>
                                    <label class="k-radio k-radio--solid k-radio--success">
                                        <input type="radio" name="radio7"> Success state
                                        <span></span>
                                    </label>
                                    <label class="k-radio k-radio--solid k-radio--brand">
                                        <input type="radio" name="radio7"> Brand state
                                        <span></span>
                                    </label>
                                </div>
                                <span class="form-text text-muted">Some help text goes here</span>
                            </div>
                        </form>

                        <!--end::Form-->
                    </div>
                </div>

                <!--end::Portlet-->
            </div>
        </div>
    </div>

    <!-- end:: Content Body -->
</div>

<!-- end:: Content -->