                        </div>
                    </div>
                    <div class="k-portlet__body">
                        <canvas id="k_chartjs_1" style="height: 300px;"></canvas>
                    </div>
                </div>

                <!--end::Portlet-->
            </div>
            <div class="col-xl-6">

                <!--begin::Portlet-->
