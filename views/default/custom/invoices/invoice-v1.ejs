                                        <tr>
                                            <td>Front-End Development</td>
                                            <td>120</td>
                                            <td>$40.00</td>
                                            <td>$4800.00</td>
                                        </tr>
                                        <tr>
                                            <td>Back-end Development</td>
                                            <td>210</td>
                                            <td>$60.00</td>
                                            <td>$12600.00</td>
